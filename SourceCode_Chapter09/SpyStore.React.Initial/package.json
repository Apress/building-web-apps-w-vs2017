    "typescript": "2.1.6",
    "tslint": "4.4.2",
    "webpack": "2.2.1",
    "webpack-dev-middleware": "1.10.0",
    "webpack-dev-server": "2.3.0",
    "webpack-md5-hash": "0.0.5",
    "webpack-merge": "2.6.1",
    "awesome-typescript-loader": "3.0.4-rc.2",
    "url-loader": "0.5.7",
    "ts-helpers": "1.1.2",
    "ts-loader": "2.0.0",
    "ts-node": "2.1.0",
